<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.product_management.InvoiceListActivity">


        <LinearLayout
            android:id="@+id/heading"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="24dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/eventButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginRight="8dp"
                android:text="Order Raised"
                android:textColor="@color/app_color"
                android:textSize="18dp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:textAlignment="center"
                android:layout_gravity="center"
                android:layout_marginRight="8dp"
                android:text="Date &amp; Time : 15-Dec-22,  3:35 PM"
                android:textSize="18dp"
                android:textStyle="italic" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableLayout
                    android:id="@+id/layoutHeading"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:stretchColumns="0,1,2">

                    <TableRow
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="32dp"

                        android:paddingBottom="4dp">

                        <TextView
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_column="0"
                            android:layout_marginRight="8dp"
                            android:paddingLeft="8dp"
                            android:text="Prodcuts"
                            android:textSize="14dp"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="Request\n Quantity"
                            android:textAlignment="textStart"
                            android:textSize="14dp"
                            android:textStyle="italic" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="Received\n Quantity"
                            android:textAlignment="textStart"
                            android:textSize="14dp"
                            android:textStyle="italic" />

                    </TableRow>

                    <TableRow
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="32dp"

                        android:paddingBottom="4dp">

                        <TextView
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_column="0"
                            android:layout_marginRight="8dp"
                            android:paddingLeft="8dp"
                            android:text="Orsaline"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="10"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="--"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                    </TableRow>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:layout_alignParentTop="true"
                        android:layout_centerVertical="true"
                        android:background="#D5D5D5" />
                    <TableRow
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="32dp"

                        android:paddingBottom="4dp">

                        <TextView
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_column="0"
                            android:layout_marginRight="8dp"
                            android:paddingLeft="8dp"
                            android:text="Entacyd plus"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="10"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="--"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                    </TableRow>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:layout_alignParentTop="true"
                        android:layout_centerVertical="true"
                        android:background="#D5D5D5" />
                    <TableRow
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="32dp"

                        android:paddingBottom="4dp">

                        <TextView
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_column="0"
                            android:layout_marginRight="8dp"
                            android:paddingLeft="8dp"
                            android:text="Paracetamol 500m..."
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="10"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="--"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                    </TableRow>
                    <View

                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:layout_alignParentTop="true"
                        android:layout_centerVertical="true"
                        android:background="#D5D5D5" />
                    <TableRow
                        android:layout_marginLeft="16dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="32dp"

                        android:paddingBottom="4dp">

                        <TextView
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_column="0"
                            android:layout_marginRight="8dp"
                            android:paddingLeft="8dp"
                            android:text="Imotil 500 mg"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="10"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:text="--"
                            android:textAlignment="center"
                            android:textSize="18dp"
                            android:textStyle="normal" />

                    </TableRow>




                </TableLayout>
            </LinearLayout>


            <View
                android:layout_marginTop="32dp"
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentTop="true"
                android:layout_centerVertical="true"
                android:background="#858585" />
            <TableLayout

                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:stretchColumns="0,1,2">




                <TableRow
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="32dp"

                    android:paddingBottom="4dp">

                    <TextView
                        android:layout_width="wrap_content"

                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_marginRight="8dp"
                        android:paddingLeft="8dp"
                        android:text="Total"
                        android:textSize="18dp"
                        android:textStyle="bold" />

                    <TextView
                        android:paddingLeft="50dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:text="40"
                        android:textAlignment="center"
                        android:textSize="18dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:text="--"
                        android:textAlignment="center"
                        android:textSize="18dp"
                        android:textStyle="normal" />

                </TableRow>




            </TableLayout>

        </LinearLayout>



        <!--    <androidx.recyclerview.widget.RecyclerView-->
        <!--        android:id="@+id/recycler_view"-->
        <!--        android:layout_width="match_parent"-->
        <!--        android:layout_height="match_parent"-->
        <!--        android:layout_below="@+id/heading"-->
        <!--        android:divider="#BABABA"-->

        <!--        android:dividerHeight="2dp"-->
        <!--        android:paddingLeft="16dp"-->
        <!--        android:paddingTop="10dp" />-->


    </RelativeLayout>
</layout>

