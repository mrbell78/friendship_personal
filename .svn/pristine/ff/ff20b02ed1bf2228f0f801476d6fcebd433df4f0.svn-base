            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="10dip"
                android:layout_marginEnd="16dp"
                android:text="@string/household_charcteristics"
                android:textSize="18sp" />

            <ngo.friendship.satellite.views.CustomAutoCompleteTextView
                android:id="@+id/act_charcteristics"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/cell_shape"
                android:drawableRight="@drawable/ic_arrow_down"
                android:hint=""
                android:imeActionLabel="Search"
                android:imeOptions="actionSearch"
                android:inputType="text"
                android:padding="16dp"
                android:popupBackground="#ffffff"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#000000" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="10dip"
                android:layout_marginEnd="16dp"
                android:text="@string/household_monthly_family_expenditure"
                android:textSize="18sp" />

            <ngo.friendship.satellite.views.CustomAutoCompleteTextView
                android:id="@+id/act_monthly_family_expenditure"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/cell_shape"
                android:drawableRight="@drawable/ic_arrow_down"
                android:hint=""
                android:imeActionLabel="Search"
                android:imeOptions="actionSearch"
                android:inputType="text"
                android:padding="16dp"
                android:popupBackground="#ffffff"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#000000" />


            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:text="@string/household_adult_women"
                android:textSize="18sp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/et_household_adult_women"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </com.google.android.material.textfield.TextInputLayout>


            <!--            <TextView-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="wrap_content"-->
            <!--                android:layout_marginTop="10dip"-->
            <!--                android:text="@string/household_adult_women"-->
