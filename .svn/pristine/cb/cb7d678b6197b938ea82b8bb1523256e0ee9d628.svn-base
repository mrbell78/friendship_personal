package ngo.friendship.satellite.model;

import java.io.Serializable;

// TODO: Auto-generated Javadoc

/**
 * The Class IndividualSalesInfo.
 */
public class IndividualSalesInfo  implements Serializable{

	/** The date. */
	private String date;
	
	/** The sale amount. */
	private double saleAmount;
	
	/**
	 * Sets the date.
	 *
	 * @param date the new date
	 */
	public void setDate(String date) {
		this.date = date;
	}
	
	/**
	 * Gets the date.
	 *
	 * @return the date
	 */
	public String getDate() {
		return date;
	}
	
	/**
	 * Sets the sale amount.
	 *
	 * @param saleAmount the new sale amount
	 */
	public void setSaleAmount(double saleAmount) {
		this.saleAmount = saleAmount;
	}
	
	/**
	 * Gets the sale amount.
	 *
	 * @return the sale amount
	 */
	public double getSaleAmount() {
		return saleAmount;
	}
}
